    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "^13.15.3",
    "@types/xlsx": "^0.0.36",
    "@vitest/ui": "^3.2.4",
    "critters": "^0.0.25",
