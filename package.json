{
  "name": "ecofor-market",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:fast": "NODE_ENV=development next dev --turbopack --port 3000 --experimental-https",
    "dev:optimized": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096' next dev --turbopack --port 3000",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "optimize-imports": "pnpm exec node scripts/optimize-imports.js",
    "fix-use-client": "pnpm exec node scripts/fix-use-client.js",
    "optimize-startup": "pnpm exec node scripts/optimize-startup.js",
    "measure-startup": "node scripts/measure-startup.js",
    "validate-env": "node scripts/validate-env.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
